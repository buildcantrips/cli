{
  "name": "@cantrips/cli",
  "version": "0.12.30",
  "description": "Cantrips cli for more descriptive builds",
  "main": "./lib/App.js",
  "repository": "https://github.com/buildcantrips/cli.git",
  "bin": {
    "cantrips": "./lib/App.js"
  },
  "author": "csaba.makara@gmail.com",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 30,
    "branches": 40,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/index.js"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "cache": false,
    "all": true
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@cantrips/basemodules": "0.12.24",
    "@cantrips/core": "0.13.20",
    "fs-extra": "^7.0.1",
    "nested-yargs": "^1.0.7",
    "simple-git": "^1.107.0",
    "tmp": "^0.0.33",
    "yargs": "^12.0.5"
  },
  "scripts": {
    "test": "mocha --require @babel/register --require mocha.env.js --timeout 10000 --recursive src",
    "test:ci": "nyc mocha --require @babel/register --require test/mocha.env.js --timeout 20000 --recursive src",
    "lint": "eslint src",
    "babel:build": "babel src -d lib",
    "babel:watch": "babel src -d lib -w"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.0.5",
    "jest-spec-reporter": "^1.0.5",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
